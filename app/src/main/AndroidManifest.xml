<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bo.vulcan.demoinvoice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoInvoice"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.invoice.list.InvoicesActivity"
            android:label="@string/title_activity_invoices"
            android:theme="@style/Theme.DemoInvoice.NoActionBar"></activity>
        <activity android:name=".ui.invoice.create.CreateInvoiceActivity" />
        <activity android:name=".ui.list.ListActivity" />
        <activity android:name=".ui.sync.parametric.SyncParametricActivity" />
        <activity android:name=".ui.sync.companyData.SyncCompanyDataActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.authenticate.AuthActivity" />
    </application>

</manifest>